{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "tsc-out",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "experimentalDecorators": true,
    "lib": ["es7", "es2017", "dom"],
    "baseUrl": ".",
    "paths": {
      "src/*": [ "./src/*" ],
      "common/*": [ "./src/common/*" ],
      "components/*": [ "./src/components/*" ],
      "common-property-connect/*": [ "./common-property-connect/src/*" ],
      "common-ts/*": [ "./common-ts/src/*" ]
    }
  },
  "include": [
    "./src/"
  ],
  "exclude": [
    "node_modules/"
  ]
}
